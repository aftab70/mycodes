#Gmail configuration using elasticsearch 6.3.1
#Also you need to set your gmail account on less security via given url https://myaccount.google.com/lesssecureapps?pli=1

xpack.notification.email.account:
    gmail_account:
        profile: gmail
        smtp:
            auth: true
            starttls.enable: true
            host: smtp.gmail.com
            port: 587
            user: xxxxxxxxxxxx
            password: xxxxxxxxxxxxx
